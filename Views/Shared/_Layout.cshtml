<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>Multi Bids</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/BridgeScenarios.styles.css" asp-append-version="true"/>
</head>
<body>
<div class="page-container">
    @{
        await Html.RenderPartialAsync("Header");
    }

    <div class="body-div">
        @RenderBody()
    </div>

    <footer class="footer">
        <a class="githubhref" href="https://github.com/monty930/BridgeScenarios">
            <div>
                monty930/BridgeScenarios
                <img
                    id="gitlog-img" class="gitlog-img"
                    src="~/assets/githublogo.png"/>
            </div>
        </a>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

    @await RenderSectionAsync("Scripts", false)
</div>
</body>
</html>